{
  "name": "vue-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "pre-commit": "lint-staged --allow-empty",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "vite": "^7.1.3",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "**/*.*": [
      "eslint --fix",
      "prettier --write src/"
    ]
  }
}
